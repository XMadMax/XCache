{
    "cache_enabled": true,
    "cache_driver": "mongodb",
    "cache_hosts": {
        "file" : {
            "path": "E:/Temp/XCache/",
            "options": false,
            "compress": true
        },
        "memcache" : {
            "host": "127.0.0.1:11211",
            "options": false,
            "compress": true
        },
        "memcached" : {
            "host": "127.0.0.1:11211",
            "options": {
                "OPT_PREFIX" : "fpsoeloc"
            },
            "compress": true
        },
        "mongodb" : {
            "host": "192.168.1.34:27017:::dbname:collection",
            "options": {
                "OPT_PREFIX" : "fpsoeloc"
            },
            "compress": true
        },
        "redis" : {
            "host": "192.168.1.23:6379",
            "options": {
                "OPT_PREFIX" : "fpsoeloc"
            },
            "compress": true
        }
    },
    "cache_logged_cookie": "islogged",
    "cache_only_not_logged_pages": false,
    "cache_get": false,
    "cache_post": false,
    "cache_pages": {
        "default": 0,
        "regexp": {
            "^/$": ["portada",600],
            "^/nuestros-sabores/": 600,
            "^/la-mesa-viajera": 600,
            "^/toda-una-vida-contigo": 10
        }
    },
    "cache_methods": {
        "default": 15,
        "regexp": {
            "^xcachetest_test": 30,
            "^xcachetest_other": 45
        },
        "xcachetest_test1": 60,
        "xcachetest_myMethod": 18
    },
    "cache_values": {
        "default": 15,
        "regexp": {
            "^varTest$": 30
        }
    }
}
